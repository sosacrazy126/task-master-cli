# Task Master CLI Environment Variables

# Project Settings
PROJECT_NAME="Task Master CLI"
PROJECT_VERSION="1.5.0"
LOG_LEVEL="info"  # Options: debug, info, warn, error
DEBUG=false

# Default Task Settings
DEFAULT_SUBTASKS=3
DEFAULT_PRIORITY="medium"  # Options: high, medium, low

# AI Provider Settings
# Choose which AI provider to use: claude or cursor
AI_PROVIDER="claude"

# Claude AI Settings (default provider)
ANTHROPIC_API_KEY="your_anthropic_api_key"
MODEL="claude-3-7-sonnet-20250219"
MAX_TOKENS=4000
TEMPERATURE=0.7

# Cursor AI Settings
CURSOR_API_KEY="your_cursor_api_key"
CURSOR_API_ENDPOINT="https://api.cursor.sh/v1/generate"
CURSOR_MODEL="cursor-3"

# Perplexity AI Settings (for research-backed features)
PERPLEXITY_API_KEY="your_perplexity_api_key"
PERPLEXITY_MODEL="sonar-medium-online"